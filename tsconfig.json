{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@src": ["./src"],
      "@redux/*": ["./src/redux/*"],
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./src/assets/*"],
      "@navigator": ["./src/navigator"],
      "@helpers": ["./src/helpers"],
      "@asyncStorage": ["./src/asyncStorage"],
      "@hooks": ["./src/hooks"],
    }
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"]
}

